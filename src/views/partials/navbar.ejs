<nav class="sidebar">
  <a href="/">
    <img class="komodo-logo" src="/assets/img/komodo-logo-horizontal-03.png">
  </a>
  <div class="sidebar-links-container">
    <div class="sidebar-title">Introduction to Komodo</div>
    <ol class="sidebar-links">
      <% topicTree.forEach((primaryTopicNav) => { %>
        <li class="sidebar-main-link">
          <a href="/primaryTopics/<%= primaryTopicNav.primaryTopicId %>"> <%= primaryTopicNav.title %> </a>
          <ol class="sidebar-links">
            <% primaryTopicNav.secondaryTopics.forEach((secondaryTopicNav) => { %>
              <li class="sidebar-second-link">
                <a href="/primaryTopics/<%= primaryTopicNav.primaryTopicId %>/secondaryTopics/<%= secondaryTopicNav.secondaryTopicId %>"> <%= secondaryTopicNav.title %> </a>
              </li>
            <% }); %>
          </ol>
        </li>
      <% }); %>
    </ol>
  </div>
</nav>
