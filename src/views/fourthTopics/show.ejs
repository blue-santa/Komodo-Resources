<!DOCTYPE html>
<html>
  <head>
      <% include ../partials/head.ejs %>
      <title><%= fourthTopic.title %>: Komodo Resources</title>
  </head>
  <body>
    <div class="flex-container">

      <main class="content">
        <div class="site-title content-flex-container unique-row breadcrumb">
            <i class="fas fa-angle-right breadcrumb-link"></i>
          <a href="/primaryTopics/<%= fourthTopic.primaryTopicId %>" class="breadcrumb-link"><%= topicTree[fourthTopic.primaryTopicId - 1].title %></a>
            <i class="fas fa-angle-right breadcrumb-link"></i>
          <a href="/primaryTopics/<%= fourthTopic.primaryTopicId %>/secondaryTopics/<%= fourthTopic.secondaryTopicId %>" class="breadcrumb-link"><%= topicTree[fourthTopic.primaryTopicId - 1].secondaryTopics[fourthTopic.secondaryTopicId - 1].title %></a>
            <i class="fas fa-angle-right breadcrumb-link"></i>
          <a href="/primaryTopics/<%= fourthTopic.primaryTopicId %>/secondaryTopics/<%= fourthTopic.secondaryTopicId %>/thirdTopics/<%= fourthTopic.thirdTopicId %>" class="breadcrumb-link"><%= topicTree[fourthTopic.primaryTopicId - 1].secondaryTopics[fourthTopic.secondaryTopicId - 1].thirdTopics[fourthTopic.thirdTopicId - 1].title %></a>
            <i class="fas fa-angle-right breadcrumb-link"></i>
          <a href="/primaryTopics/<%= fourthTopic.primaryTopicId %>/secondaryTopics/<%= fourthTopic.secondaryTopicId %>/thirdTopics/<%= fourthTopic.thirdTopicId %>/fourthTopics/<%= fourthTopic.id %>" class="breadcrumb-link"><%= fourthTopic.title %></a>
        </div>
        <div class="site-content">
          <hr>
          <p><%- fourthTopic.content %></p>
          <hr>
          <h1>Fifth Topics</h1>
          <ul class="list-group">
            <% fourthTopic.fifthTopics.forEach((fifthTopic) => { %>
              <li class="topic-list">
                <a href="/primaryTopics/<%= fifthTopic.primaryTopicId %>/secondaryTopics/<%= fifthTopic.secondaryTopicId %>/thirdTopics/<%= fifthTopic.thirdTopicId %>/fourthTopics/<%= fifthTopic.fourthTopicId %>/fifthTopics/<%= fifthTopic.id %>"><%= fifthTopic.title %></a>
              </li>
            <% }); %>
          </ul>
          <hr>
          <div class="site-footer">
            <h3><span class="copyright-symbol">&#9400;</span><%= (new Date()).getFullYear() %> Komodo Platform</h3>
          </div>
        </div>
      </main>
    </div>
    <% include ../partials/baseScripts.ejs %>
  </body>
</html>
